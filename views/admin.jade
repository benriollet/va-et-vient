doctype html
html
  head
    link(rel='stylesheet', href='http://fonts.googleapis.com/css?family=Open+Sans')
    link(rel='stylesheet', href='/css/style.css')
    script(src='/js/socket.io.min.js')
    script(src='/js/jquery-1.11.1.js')
    script(src='/js/admin.js')
    title #{pageData.title}
  body
    header.page-header
      h1 #{pageData.title}
    div.row
      h2 new session
      label session name
      input(type="textfield" class="session-name")
      br
      label number of capture
      input(type="textfield" class="capture-lenght" value="3")
      br
      button.create-session.btn-medium create session